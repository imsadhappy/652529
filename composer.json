{
    "name": "imsadhappy/groupbwt-652529-php-symfony-software-engineer-test-assignment",
    "type": "project",
    "description": "Commission Calculator",
    "license": "MIT",
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "run:tests": "@php ./vendor/bin/phpunit --testdox tests",
        "run:app": "@php app.php input.txt",
        "post-install-cmd": "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    },
    "require": {
        "php": "^8.1.25",
        "guzzlehttp/guzzle": "^7.0",
        "brick/money": "^0.9.0"
    }
}
